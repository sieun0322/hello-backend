{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(go run *)",
      "Bash(go build *)",
      "Bash(go test *)",
      "Bash(go mod *)",
      "Bash(docker-compose *)",
      "Bash(docker build *)"
    ],
    "deny": ["Read(./.env)", "Bash(rm -rf *)"]
  },
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  }
}
